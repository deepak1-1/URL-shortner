const mongoose = require('mongoose');
const Schema = mongoose.Schema;

const urlSchema = new Schema({

    url: {
        type: String
    },
    shortId: {
        type: String
    },
    createdBy: {
        type: Array
    },
    createdTimestamp: {
        type: String
    },
    attempts: {
        type: String
    },
    validUptoTime: {
        type: Object
    }

}, {timestamps : true});

const urlDataModel = mongoose.model('urlData', urlSchema);
module.exports = urlDataModel;